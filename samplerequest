curl --location --request POST 'http://localhost:4000/' \
--header 'Content-Type: application/json' \
--data-raw '{"query":"query GetAllAlbums ($payload:inputAlbum) {\r\n  albums (payload: $payload) {\r\n    title \r\n    year\r\n    artist {\r\n      name\r\n    }\r\n    tracks {\r\n      title \r\n    }\r\n    genre\r\n  }\r\n}","variables":{"payload":{"genre":"Pop","artist":{"name":"Michael Jackson"}}}}'